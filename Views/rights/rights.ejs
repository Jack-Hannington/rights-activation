<% include ../partials/header %>

<div class="container">
  <div class="client-about-row">
    <h2>All Client rights</h2>
    <input type="text" class="search-bar" id="rights_search" onkeyup="searchrights()" placeholder="Search rights by category..." title="Search rights">
   
    <table id="rightsTable" style="width:100%;">
        <tr>
          <td>Category</td>
          <td>Detail</td>
          <td>Due Date</td>
          <td>Progress</td>
        </tr>
        <% rights.forEach(function(right){ %>
        <tr>
          <td> <%= right.category%> </td>
          <td> <%= right.description %></td>
          <td> <%= right.due_date.toLocaleDateString('en-GB') %></td>
          <td> <%= right.complete %></td>
        </tr>
        <% }) %>
      </table>
 
  </div> 
</div>





<% include ../partials/footer %>