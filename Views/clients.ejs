<% include partials/header %>

    <div class="container">
        <div class="row margin-top-20">
            <div class="col-lg-12">
                <h3 style="display:inline;">Dashboard</h3>
                <a title="add new client" href="/clients/new"><button class="move-right">New Client</button></a>
            </div>
        </div>
        <div class="row">
            <% clients.forEach(function(client){ %>
                <div class="col-xs-12 col-sm-12 col-md-6 col-lg-4">
                    <a class="client-card-link" href="/clients/<%= client._id %>">
                        <div class="client-card-square">
                            <div class="row general-row">

                                <div class="col-12">
                                    <p class="values">
                                        <%= client.name %>
                                    </p>
                                    <p>
                                        <%= client.designation %>
                                    </p>
                                </div>
                            </div>
                            <div class="row general-row client-card-details">
                                <div class="col">
                                    <p> <span class="value-description"> Value</span> Â£
                                        <%= client.value %>
                                    </p>
                                    <p><span class="value-description"> Start:</span> <span class="value-description"> |   Expiry: </span>

                                    </p>

                                </div>
                            </div>

                        </div>
                    </a>
                </div>
                <% }) %>
        </div>
    </div>







    <% include partials/footer %>